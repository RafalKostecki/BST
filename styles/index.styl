//Stylus preprocessor, SMACSS methodology
//Mixins
vendor(prop, args)
    -webkit-{prop} args
    -moz-{prop} args
    {prop} args
border-radius()
    vendor('border-radius', arguments)
box-shadow()
    vendor('box-shadow', arguments)
//Colors
darkBlue = #2f2f83
//Base styles
html, body
    font-family 'Arial', sans-serif
    margin 0
    padding 0
    width 100%
    height 100%
    font-weight normal
    background #111
    color #eee
//Classes
